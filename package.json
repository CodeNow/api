  "devDependencies": {
    "nodemon": "~0.7.8",
    "jshint": "~2.4.1",
    "console-trace": "git://github.com/numbcoder/console-trace.git",
    "findit": "~1.1.0",
    "nock": "~0.34.0",
    "jsdoc": "~3.3.0-alpha5",
    "lab": "~3.1.1",
    "krain": "git://github.com/CodeNow/krain.git",
    "debug": "~0.7.4",
    "substream": "0.0.2"
  },
  "engine": "node >= 0.10.5",
  "scripts": {
    "docs": "jsdoc -r -d docs lib/ && if [ $CIRCLE_ARTIFACTS ]; then cp -r docs $CIRCLE_ARTIFACTS; fi",
    "fixme": "grep -nR FIXME lib test",
    "todo": "grep -nR TODO lib test",
    "start": "NODE_PATH=./lib node server.js",
    "watch": "NODE_PATH=./lib nodemon -w lib -w configs -q server.js",
    "coverage": "NODE_PATH=./lib NODE_ENV=test lab -vl -m 500 -e test -c -r html test/*.js > coverage.html && if [ $CIRCLE_ARTIFACTS ]; then cp coverage.html $CIRCLE_ARTIFACTS; fi",
    "test-watch": "killall node; nodemon -w lib -w test -w configs --exec npm test",
    "test": "echo -ne \"\\033]50;ClearScrollback\\a\"; npm run lint && NODE_PATH=./lib NODE_ENV=test lab -v -l -m 500 -e test test/*.js",
    "client-version": "node ./scripts/client-version-bump.js",
    "unit": "NODE_PATH=./lib lab -e test -vG unit/",
    "clean": "rm -rf ./coverage",
    "lint": "jshint ."
  }
}
