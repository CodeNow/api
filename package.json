    "nodemon": "^1.2.1",
    "substream": "0.0.2"
  },
  "engine": "node >= 0.10.5",
  "scripts": {
    "clear": "echo -ne \"\\033]50;ClearScrollback\\a\";",
    "docs": "./node_modules/.bin/jsdoc -r -d docs lib/ && if [ $CIRCLE_ARTIFACTS ]; then cp -r docs $CIRCLE_ARTIFACTS; fi",
    "fixme": "grep -nR FIXME lib test",
    "todo": "grep -nR TODO lib test",
    "start": "NODE_PATH=./lib node server.js",
    "watch": "NODE_PATH=./lib ./node_modules/.bin/nodemon -w lib -w configs server.js",
    "coverage": "NODE_PATH=./lib NODE_ENV=test ./node_modules/.bin/lab -vl -m 500 -e test -c -r html test/*.js > coverage.html && if [ $CIRCLE_ARTIFACTS ]; then cp coverage.html $CIRCLE_ARTIFACTS; fi",
    "test-watch": "killall node; ./node_modules/.bin/nodemon -w package.json -w test -w lib -w node_modules/runnable/lib/models --exec npm test",
    "test": "npm run clear; npm run lint; NODE_PATH=./lib NODE_ENV=test ./node_modules/.bin/lab -v -l -m 500 -e test test/*.js",
    "client-version": "node ./scripts/client-version-bump.js",
    "unit": "NODE_PATH=./lib ./node_modules/.bin/lab -e test -vG unit/",
    "clean": "rm -rf docs coverage.html node_modules",
    "lint": "./node_modules/.bin/jshint ."
  }
}
