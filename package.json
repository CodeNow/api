  "scripts": {
    "test-watch": "echo use 'bdd-watch' or 'unit-watch'",
    "_bdd": "NODE_PATH=./lib NODE_ENV=test lab --verbose --leaks",
    "bdd": "CIRCLE_NODE_TOTAL=1 CIRCLE_NODE_INDEX=0 ./scripts/npm-bdd-split-files.sh",
    "bdd-watch": "killall node; nodemon -w package.json -w test -w lib -w node_modules/runnable/lib/models -w node_modules/runnable/lib/collections --exec 'npm run' bdd-whitelist",
    "bdd-debug": "npm run clear; NODE_PATH=./lib NODE_ENV=test node-debug ./node_modules/.bin/lab -v -l -m 36000 -e test `cat test/whitelist.txt`",
    "bdd-whitelist": "npm run clear; NODE_PATH=./lib NODE_ENV=test lab -v -l -m 200 -e test `cat test/whitelist.txt`",
    "clear": "echo -ne \"\\033]50;ClearScrollback\\a\";",
    "client-version": "node ./scripts/client-version-bump.js",
    "docs": "jsdoc -r -d docs lib/ && if [ $CIRCLE_ARTIFACTS ]; then cp -r docs $CIRCLE_ARTIFACTS; fi",
    "fixme": "grep -nR FIXME lib test",
    "todo": "grep -nR TODO lib test",
    "start": "NODE_PATH=./lib node app.js",
    "start-debug": "NODE_PATH=./lib node --debug app.js",
    "watch": "NODE_PATH=./lib nodemon -w lib -w configs app.js",
    "coverage": "NODE_PATH=./lib NODE_ENV=test lab -vl -m 500 -e test -c -r html test/*.js > coverage.html && if [ $CIRCLE_ARTIFACTS ]; then cp coverage.html $CIRCLE_ARTIFACTS; fi",
    "test": "npm run clear; npm run lint && npm run bdd && npm run unit",
    "unit": "npm run clear; NODE_PATH=./lib lab --leaks --verbose unit/",
    "unit-watch": "killall node; nodemon -w package.json -w unit -w lib -w node_modules/runnable/lib/models --exec 'npm run' unit",
    "unit-debug": "npm run clear; NODE_PATH=./lib NODE_ENV=test node-debug ./node_modules/.bin/lab -l -e test -vG unit/",
    "clean": "rm -rf docs coverage.html node_modules",
    "lint": "jshint .",
    "cayley": "./test/fixtures/graphs/cayley/cayley_osx http --host=0.0.0.0 --config=./test/fixtures/graphs/cayley/local.cfg --assets=./test/fixtures/graphs/cayley"
  }
