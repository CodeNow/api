  "scripts": {
    "_bdd": "NODE_PATH=./lib NODE_ENV=test lab --assert code --verbose --leaks --context-timeout 80000 --timeout 80000",
    "bdd": "CIRCLE_NODE_TOTAL=1 CIRCLE_NODE_INDEX=0 ./scripts/npm-bdd-split-files.sh",
    "bdd-debug": "npm run clear; NODE_PATH=./lib NODE_ENV=test node-debug ./node_modules/.bin/lab -a code -v -l --context-timeout 80000 --timeout 80000 -e test `cat test/whitelist.txt`",
    "bdd-watch": "killall node; nodemon -w package.json -w test -w lib -w node_modules/runnable/lib/models -w node_modules/runnable/lib/collections --exec 'npm run' bdd-whitelist",
    "bdd-whitelist": "npm run clear; NODE_PATH=./lib NODE_ENV=test lab  --context-timeout 80000 --timeout 80000 --assert code --verbose --leaks `cat test/whitelist.txt`",
    "integration": "npm run clear; NODE_PATH=./lib NODE_ENV=test lab  --context-timeout 80000 --timeout 80000 --assert code --verbose --leaks test/integration",
    "clean": "rm -rf docs coverage.html node_modules",
    "clear": "echo -ne \"\\033]50;ClearScrollback\\a\";",
    "client-version": "node ./scripts/client-version-bump.js",
    "coverage": "NODE_PATH=./lib NODE_ENV=test lab -vl -m 60000 -e test -c -r html test/*.js > coverage.html && if [ $CIRCLE_ARTIFACTS ]; then cp coverage.html $CIRCLE_ARTIFACTS; fi",
    "docs": "jsdoc -r -d docs lib/ && if [ $CIRCLE_ARTIFACTS ]; then cp -r docs $CIRCLE_ARTIFACTS; fi",
    "fixme": "grep -nR FIXME lib test",
    "format": "standard --fix --verbose",
    "generate-toc": "doctoc ./README.md",
    "generate-whitelist": "find ./test -path ./test/functional/fixtures -prune -o -name '*.js' -print | sort > test/whitelist.txt",
    "lint": "standard --verbose",
    "migrate-down": "NODE_PATH=./lib node ./mm-migrations/migrator-down.js",
    "migrate-up": "NODE_PATH=./lib node ./mm-migrations/migrator-up.js",
    "migrate-and-start": "npm run migrate-up; NODE_PATH=./lib node --abort-on-uncaught-exception index.js",
    "start": "NODE_PATH=./lib node --abort-on-uncaught-exception index.js",
    "start-debug": "NODE_PATH=./lib node --debug index.js",
    "todo": "grep -nR TODO lib test",
    "test": "npm run clear; npm run lint && npm run bdd && npm run unit",
    "test-watch": "echo use 'bdd-watch' or 'unit-watch'",
    "unit": "npm run clear; NODE_PATH=./lib lab --assert code --leaks --verbose unit/",
    "unit-debug": "npm run clear; NODE_PATH=./lib NODE_ENV=test node-debug ./node_modules/.bin/lab -l -e test -vG unit/",
    "unit-watch": "nodemon -w package.json -w unit -w lib -w node_modules/runnable/lib/models --exec 'npm run' unit",
    "watch": "NODE_PATH=./lib nodemon -w lib -w configs index.js"
  },
  "pre-commit": [
    "lint"
  ]
}
